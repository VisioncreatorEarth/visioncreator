{
  "name": "visioncreator",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "generate": "turbo run generate",
    "api:build": "turbo run build --filter=api",
    "api:start": "turbo run start --filter=api"
  },
  "devDependencies": {
    "turbo": "^2.0.3"
  },
  "engines": {
    "node": ">=20.12.2"
  },
  "packageManager": "npm@10.5.0",
  "volta": {
    "node": "20.12.2",
    "npm": "10.5.0"
  }
}
{
  "name": "visioncreator",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "generate": "turbo run generate",
    "api:build": "turbo run build --filter=api",
    "api:start": "turbo run start --filter=api"
  },
  "devDependencies": {
    "turbo": "^2.0.3"
  },
  "engines": {
    "node": ">=22.2.0"
  },
  "packageManager": "npm@10.7.0",
  "volta": {
    "node": "22.2.0",
    "npm": "10.7.0"
  }
}
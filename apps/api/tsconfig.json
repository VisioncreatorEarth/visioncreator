{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonjs",
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true
	},
	"include": ["**/*.ts", ".wundergraph/wundergraph.config.ts", ".wundergraph/wundergraph.operations.ts", ".wundergraph/wundergraph.server.ts"],
	"exclude": ["node_modules"]
}
